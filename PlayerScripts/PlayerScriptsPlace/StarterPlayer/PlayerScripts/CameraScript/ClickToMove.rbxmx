&#9;&#9;&#9;&#9;&#9;&#9;this.pathResult = pathResult
&#9;&#9;&#9;&#9;&#9;&#9;if this.pathResult then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;this.pointList = this.pathResult:GetPointCoordinates()
