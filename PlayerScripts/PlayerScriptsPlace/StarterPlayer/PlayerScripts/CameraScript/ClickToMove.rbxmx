local function AutoJumper()
&#9;local this = {}
&#9;local running = false
&#9;local runRoutine = nil
&#9;
&#9;function this:Run()
&#9;&#9;running = true
&#9;&#9;local thisRoutine = nil
&#9;&#9;thisRoutine = coroutine.create(function()
&#9;&#9;&#9;while running and thisRoutine == runRoutine do
&#9;&#9;&#9;&#9;this:Step()
&#9;&#9;&#9;&#9;wait()
&#9;&#9;&#9;end
&#9;&#9;end)
